{
  "labels": {
    "ask": {
      "schema": "https://tbd/website/tbdex/ask"
    },
    "offer": {
      "schema": "https://tbd/website/tbdex/offer"
    },
    "fulfillment": {
      "schema": "https://tbd/website/tbdex/fulfillment"
    }
  },
  "records": {
    "ask": {
      "allow": {
        "anyone": {
          "to": [
            "write"
          ]
        }
      },
      "records": {
        "offer": {
          "allow": {
            "recipient": {
              "of": "ask",
              "to": [
                "write"
              ]
            }
          },
          "records": {
            "fulfillment": {
              "allow": {
                "recipient": {
                  "of": "ask/offer",
                  "to": [
                    "write"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}