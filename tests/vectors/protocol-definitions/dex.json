{
  "protocol": "http://dex.xyz",
  "published": true,
  "types": {
    "ask": {
      "schema": "https://tbd/website/tbdex/ask",
      "dataFormats": [
        "application/json"
      ]
    },
    "offer": {
      "schema": "https://tbd/website/tbdex/offer",
      "dataFormats": [
        "application/json"
      ]
    },
    "fulfillment": {
      "schema": "https://tbd/website/tbdex/fulfillment",
      "dataFormats": [
        "application/json"
      ]
    }
  },
  "structure": {
    "ask": {
      "$actions": [
        {
          "who": "anyone",
          "can": "write"
        }
      ],
      "offer": {
        "$actions": [
          {
            "who": "recipient",
            "ofRecord": {
              "atPath": "ask",
              "inGroup": "ancestors"
            },
            "can": "write"
          }
        ],
        "fulfillment": {
          "$actions": [
            {
              "who": "recipient",
              "ofRecord": {
                "atPath": "ask/offer",
                "inGroup": "ancestors"
              },
              "can": "write"
            }
          ]
        }
      }
    }
  }
}
