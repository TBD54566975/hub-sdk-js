{
  "protocol": "http://recipient-can-protocol.xyz",
  "published": true,
  "types": {
    "post": {},
    "tag": {}
  },
  "structure": {
    "post": {
      "$actions": [
        {
          "who": "recipient",
          "can": "co-update"
        },
        {
          "who": "recipient",
          "can": "co-delete"
        }
      ],
      "tag": {
        "$actions": [
          {
            "who": "recipient",
            "of": "post",
            "can": "co-update"
          },
          {
            "who": "recipient",
            "of": "post",
            "can": "co-delete"
          }
        ]
      }
    }
  }
}