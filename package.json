{
  "name": "dwn-sdk",
  "version": "0.1.0",
  "description": "A reference implementation of https://identity.foundation/decentralized-web-node/spec/",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "node": ">= 14"
  },
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "@comment files": [
    "the files property informs npm about which files we want to include in our published package.",
    "dist will include all transpiled js. There's no point in including .ts files"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'dist/cjs' --source-maps",
    "build:esm": "cross-env BABEL_ENV=esm babel src --extensions '.ts' --out-dir 'dist/esm' --source-maps",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build:declarations": "tsc -p tsconfig.json",
    "build": "npm-run-all -l clean -p build:esm build:cjs build:bundles build:declarations",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "lint:fix": "eslint . --ext .ts --fix",
    "test:node": "ts-mocha --type-check tests/**/*.ts",
    "test:browser": "cross-env BABEL_ENV=test karma start karma.conf.cjs",
    "test:browser-debug": "cross-env BABEL_ENV=test karma start karma.conf.debug.cjs"
  },
  "dependencies": {
    "@ipld/dag-cbor": "7.0.1",
    "@noble/ed25519": "^1.6.0",
    "@swc/helpers": "0.3.8",
    "@types/ms": "0.7.31",
    "ajv": "8.11.0",
    "base64url": "3.0.1",
    "buffer": "6.0.3",
    "chai-as-promised": "7.1.1",
    "date-fns": "2.28.0",
    "ms": "2.1.3",
    "multiformats": "9.6.4"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@babel/runtime": "7.17.8",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.5",
    "@types/lodash": "4.14.179",
    "@types/mocha": "9.1.0",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.13.0",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "eslint": "8.11.0",
    "karma": "6.3.17",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-rollup-preprocessor": "7.0.8",
    "mocha": "9.2.1",
    "mockdate": "3.0.5",
    "ms": "2.1.3",
    "npm-run-all": "4.1.5",
    "process": "0.11.10",
    "rimraf": "3.0.2",
    "rollup": "2.70.0",
    "rollup-plugin-polyfill-node": "0.8.0",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "13.0.1",
    "ts-mocha": "9.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.5.5",
    "typescript-json-schema": "0.53.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TBD54566975/dwn-sdk-js.git"
  },
  "author": "Moe Jangda <moe@squareup.com> (https://twitter.com/moe_jangda)",
  "contributors": [
    {
      "name": "Daniel Buchner",
