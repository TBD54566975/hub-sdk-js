{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://identity.foundation/dwn/json-schemas/records-filter.json",
  "type": "object",
  "minProperties": 1,
  "properties": {
    "protocol": {
      "type": "string"
    },
    "protocolPath": {
      "type": "string"
    },
    "attester": {
      "$ref": "https://identity.foundation/dwn/json-schemas/defs.json#/definitions/did"
    },
    "recipient": {
      "$ref": "https://identity.foundation/dwn/json-schemas/defs.json#/definitions/did"
    },
    "contextId": {
      "type": "string"
    },
    "schema": {
      "type": "string"
    },
    "parentId": {
      "type": "string"
    },
    "dataFormat": {
      "type": "string"
    },
    "dateCreated": {
      "type": "object",
      "minProperties": 1,
      "additionalProperties": false,
      "properties": {
        "from": {
          "$ref": "https://identity.foundation/dwn/json-schemas/defs.json#/definitions/date-time"
        },
        "to": {
          "$ref": "https://identity.foundation/dwn/json-schemas/defs.json#/definitions/date-time"
        }
      }
    }
  }
}